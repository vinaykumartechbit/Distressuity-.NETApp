<body ng-controller="mainController">
    

    <div class="header navbar-fixed-top" ng-cloak>
        <div class="container-fluid">
            <div class="row">
                <div class="col-md-12">
                    <div class="row">
                        <nav class="custom-nav navbar navbar-inverse">
                            <div class="container-fluid">
                                <div class="navbar-header">
                                    <button type="button" class="navbar-toggle" data-toggle="collapse" data-target="#myNavbar">
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                        <span class="icon-bar"></span>
                                    </button>
                                    <a class="navbar-brand" href="#"><img src="images/logo.png" width="150"></a>
                                </div>
                                <div class="collapse navbar-collapse" id="myNavbar">
                                    <div class="row">
                                        <div class="col-md-4 col-sm-6">
                                            <ul class="nav navbar-nav">
                                                <li ng-if="$root.userRole.toLowerCase() != 'admin'">
                                                    <a class="newProject" href="#project"><strong>Start Your Idea</strong></a>
                                                </li>
                                                <li ng-if="$root.userRole.toLowerCase() == 'admin'">
                                                    <a href="#allProjects"> All Projects</a>
                                                </li>
                                                <li class="dropdown">
                                                    <a class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                                                        <span ng-bind="SelectedIndustry"></span>
                                                        <span class="caret"></span>
                                                    </a>

                                                    <ul class="dropdown-menu navbar-ul-height">
                                                        <li>
                                                            <a href="" ng-click="filterProjectByIndustryId(0)">All</a>
                                                        </li>
                                                        <li ng-repeat="data in LayoutIndustries" class="border-top">
                                                            <a href="" ng-click="filterProjectByIndustryId(data); SelectedIndustry = 'Test'">{{data.Name}}</a>
                                                        </li>
                                                    </ul>
                                                </li>
                                            </ul>
                                        </div>
                                        <div class="col-md-4 col-sm-3">
                                            <div class="logo text-center">
                                                <a href="#/"><img src="images/logo.png" width="200"></a>
                                            </div>
                                        </div>
                                        <div class="col-md-4 col-sm-3">
                                            <ul class="nav navbar-nav navbar-right" data-ng-if="isAuthenticated.isAuth">
                                               
                                                <li>
                                                    <a href="#messages">
                                                        <span class="fa fa-bell"></span>
                                                        <span class="new-messages-count" ng-if="$root.NewMessagesCount > 0" ng-bind="$root.NewMessagesCount"></span>
                                                    </a>
                                                </li>
                                                <li class="dropdown user user-menu">
                                                    <a href="#" prevent-default class="dropdown-toggle" data-toggle="dropdown">

                                                        <img class="user-image" ng-src="/api/Public/GetFileStream?filename={{User.PictureUrl}}&id=0&w=35&h=35"
                                                             onerror="this.onerror = null; this.src = '/images/error-image.jpg'; this.style = 'width: 35px; height:35px'" data-toggle="dropdown"
                                                             alt="User Image" role="button" aria-haspopup="true" aria-expanded="true">
                                                        @*<span class="hidden-xs">User</span>*@
                                                        <span class="caret"></span>
                                                    </a>
                                                    <ul class="dropdown-menu">

                                                        <li class="user-header">
                                                            <img class="img-circle" ng-src="/api/Public/GetFileStream?filename={{User.PictureUrl}}&id=0&w=100&h=100"
                                                                 onerror="this.onerror = null; this.src = '/images/error-image.jpg'; this.style = 'width: 100px; height:100px'" data-toggle="dropdown"
                                                                 role="button" aria-haspopup="true" aria-expanded="true">
                                                            <p> Hi, {{User.FirstName}} {{User.LastName}}</p>
                                                            <a href="#messages">Messages</a>
                                                            <a href="#reports" ng-if="$root.userRole.toLowerCase() == 'admin'">Reports</a>
                                                            <a href="#myprojects" ng-if="$root.userRole.toLowerCase() != 'admin'">My Projects</a>
                                                            <a href="#mypayments" ng-if="$root.userRole.toLowerCase() != 'admin'">My Investments</a>
                                                        </li>

                                                        <li class="user-footer">
                                                            <div class="pull-left">
                                                                <a href="#profile" class="btn btn-default btn-flat">My Profile</a>
                                                            </div>
                                                            <div class="pull-right">
                                                                <a href="#" prevent-default class="btn btn-default btn-flat" data-ng-click="logOut()">Sign Out</a>
                                                            </div>
                                                        </li>
                                                    </ul>
                                                </li>
                                            </ul>
                                            <ul class="nav navbar-nav navbar-right" data-ng-if="!isAuthenticated.isAuth">
                                                <li><a href="#signup"><span class="glyphicon glyphicon-user"></span> Sign Up</a></li>
                                                <li><a href="#login"><span class="glyphicon glyphicon-log-in"></span> Login</a></li>
                                            </ul>
                                            <form class="navbar-form navbar-right" role="search" ng-show="$root.IsSearch">
                                                <div class="input-group">

                                                    <input type="text" class="form-control" ng-change="searchProjects();"
                                                           ng-model="SearchProject" name="SearchProject"
                                                           placeholder="Search Projects" autocomplete="off">
                                                    <span class="input-group-btn">
                                                        <button type="reset" class="btn btn-default" ng-click="clearSearch();">
                                                            <span class="glyphicon glyphicon-remove">
                                                                <span class="sr-only">Close</span>
                                                            </span>
                                                        </button>
                                                        <button type="submit" class="btn btn-default" ng-click="clearSearch();">
                                                            <span class="glyphicon glyphicon-search">
                                                                <span class="sr-only">Search</span>
                                                            </span>
                                                        </button>
                                                    </span>
                                                </div>
                                            </form>
                                        </div>
                                    </div>
                        </nav>
                    </div>
                </div>
            </div>
        </div>
    </div>

    @*<div class="centerDiv">*@
    <div ng-view="">
        @RenderBody()
    </div>
    @*</div>*@

    <div id="preloader" class="display-none">
